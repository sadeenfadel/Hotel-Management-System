@model IEnumerable<ROYALHOTEL.Models.Feature>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout1.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Featuretext)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Featuretext)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Featureid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Featureid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Featureid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
